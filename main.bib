@article{morenoOpenEndedFraternalTransitions2019,
  title = {Toward {{Open-Ended Fraternal Transitions}} in {{Individuality}}},
  author = {Moreno, Matthew Andres and Ofria, Charles},
  date = {2019-05-01},
