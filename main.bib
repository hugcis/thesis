@article{toffoliEntropyHonest2016,
  title = {Entropy? {{Honest}}!},
  shorttitle = {Entropy?},
  author = {Toffoli, Tommaso},
  date = {2016-06-30},
  journaltitle = {Entropy},
  shortjournal = {Entropy},
  volume = {18},
  number = {7},
  eprint = {1705.02223},
  eprinttype = {arxiv},
  pages = {247},
