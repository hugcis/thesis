@online{hatamizadehGlobalContextVision2022,
  title = {Global {{Context Vision Transformers}}},
  author = {Hatamizadeh, Ali and Yin, Hongxu and Kautz, Jan and Molchanov, Pavlo},
  date = {2022-06-20},
  eprint = {2206.09959},
