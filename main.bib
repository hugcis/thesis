@article{chenReprogrammableMechanicalMetamaterial2021,
  title = {A Reprogrammable Mechanical Metamaterial with Stable Memory},
  author = {Chen, Tian and Pauly, Mark and Reis, Pedro M.},
  date = {2021-01},
  journaltitle = {Nature},
  volume = {589},
  number = {7842},
