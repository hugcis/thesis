@article{kitayamaNovelFrontierPhotonics2019,
  title = {Novel Frontier of Photonics for Data Processing—{{Photonic}}           Accelerator},
  author = {Kitayama, Ken-ichi and Notomi, Masaya and Naruse, Makoto and Inoue, Koji and Kawakami, Satoshi and Uchida, Atsushi},
