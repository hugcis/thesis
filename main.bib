  author = {Moore, Jared M. and Stanton, Adam},
  date = {2020-07-01},
  journaltitle = {Artificial Life Conference Proceedings},
  shortjournal = {Artificial Life Conference Proceedings},
  volume = {32},
  pages = {719--726},
  publisher = {{MIT Press}},
